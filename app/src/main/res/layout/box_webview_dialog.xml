<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    android:id="@+id/box_webview_dialog_layout">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="@color/white">
        <TextView
            android:id="@+id/box_webview_dialog_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textStyle="bold"
            android:layout_centerInParent="true"
            android:textColor="@color/grey_66"
            android:text="暂无标题"/>
        <ImageView
            android:id="@+id/box_webview_dialog_btn"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginRight="10dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_gravity="right"
            android:background="@drawable/ic_webview_close"/>
    </RelativeLayout>
    <WebView
        android:id="@+id/box_webview_dialog_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>
